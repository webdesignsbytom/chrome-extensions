{
    "manifest_version": 3,
    "name": "My First Chrome Extension",
    "version": "1.0",
    "description": "A simple Chrome extension example.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "./src/popup.html",
      "default_icon": {
        "16": "./src/assets/images/icon16.png",
        "48": "./src/assets/images/icon48.png",
        "128": "./src/assets/images/icon128.png"
      }
    }
  }
  